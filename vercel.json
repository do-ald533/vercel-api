{
    "version": 2,
    "builds": [
        {
            "src": "src/main.ts",
            "use": "@vercel/node"
        }
    ],
    "env": {
        "MONGO_URI": "@mongo_uri",
        "TS_ENTITIES": "@ts_entities",
        "ENTITES": "@entities"
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET"
            ]
        }
    ]
}